@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error Page";
}
<hgroup>
    <h1>Error.</h1>
    <h2>An error occurred while processing your request.</h2>
    @if (Model != null && Model.Exception != null)
    {
        <h4>Error Details : @Model.Exception.Message</h4>
    }
</hgroup>
